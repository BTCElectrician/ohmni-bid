# OpenAI API Configuration
OPENAI_API_KEY=

# Model to use (must support web_search tool)
# Options: gpt-4o, gpt-4o-mini, o1, o1-mini
OHMNI_OPENAI_MODEL=gpt-4o

# Reasoning effort for models that support it
OHMNI_OPENAI_REASONING_EFFORT=high

# Maximum sources to process per daily run
OHMNI_MAX_SOURCES=20

# Enable two-stage pipeline (fast scan + deep analysis)
OHMNI_TWO_STAGE=false

# Override run date for backfill (YYYY-MM-DD format)
OHMNI_RUN_DATE=
